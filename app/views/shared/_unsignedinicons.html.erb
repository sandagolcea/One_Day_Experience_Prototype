<a href='/' >
<icon class="icons jsicons" >
	<img class="icon" src="/images/icons/arrow-button-13.svg">
	<p class="links">Home</p>
</icon>
</a>

<%= link_to about_path do%>
	<icon id="about-icon" class="icons jsicons" >
		<img class="icon" src="/images/icons/iconography-06.svg"/>
		<p class="links">About</p>
	</icon>
<% end %>

<% if !professional_signed_in? && !student_signed_in? && !admin_signed_in? %>
	<%= link_to sign_in_gate_path do%>
		<icon id="how-wide" class="icons--signin jsicons" >
			<img class="icon" src="/images/icons/iconography-05.svg"/>
			<p class="links">Sign in</p>
		</icon>
	<% end %>
<% end %>

<% if professional_signed_in? || student_signed_in? || admin_signed_in? %>
	<%= link_to destroy_professional_session_path, method: :delete do%>
		<icon id="professional_sign_out" class="icons jsicons" >
			<img class="icon" src="/images/icons/iconography-09.svg">
			<p class="links">Sign out</p>
		</icon>
	<% end %>
<% end %>

<% if student_signed_in? %>
	<%= link_to favorites_path do%>
		<icon id="favorites-icon" class="icons jsicons" >
			<img class="icon" src="/images/icons/iconography-08.svg"/>
			<p class="links">Favorites</p>
		</icon>
	<% end %>
<% end %>


<%= link_to search_path do%>
	<icon id="search-icon" class="icons jsicons" >
		<img class="icon" src="/images/icons/iconography-07.svg"/>
		<p class="links">Search</p>
	</icon>
<% end %>

<%= link_to profiles_path do%>
	<icon id="browse-profiles-icon" class="icons jsicons" >
		<img class="icon" src="/images/icons/iconography-06.svg"/>
		<p class="links">Browse Profiles</p>
	</icon>
<% end %>